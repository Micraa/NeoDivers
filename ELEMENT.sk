function element_score(n: number) :: string:
	if {_n} > 1:
		return "&dS++ &7(mixed only)"
		stop
	if {_n} > 0.95:
		return "&dS+"
		stop
	if {_n} > 0.9:
		return "&dS"
		stop
	if {_n} > 0.8:
		return "&eA+"
		stop
	if {_n} > 0.7:
		return "&eA"
		stop
	if {_n} > 0.6:
		return "&eA-"
		stop
	if {_n} > 0.5:
		return "&bB+"
		stop
	if {_n} > 0.4:
		return "&bB"
		stop
	if {_n} > 0.3:
		return "&bB-"
		stop
	if {_n} > 0.2:
		return "&aC+"
		stop
	if {_n} > 0.1:
		return "&aC"
		stop
	else:
		return "&aC-"
		stop

