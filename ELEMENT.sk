function element_rise(mode: number, item.1: itemtype, item.2: itemtype) :: itemtype:
	"%tag ""star"" of nbt of {_item.1}%" parsed as number is "%tag ""star"" of nbt of {_item.2}%" parsed as number
	"%tag ""id"" of nbt of {_item.1}%" is "%tag ""id"" of nbt of {_item.2}%"
	set {_star} to "%tag ""star"" of nbt of {_item.1}%" parsed as number
	set {_id} to "%tag ""id"" of nbt of {_item.1}%" parsed as number



	if {_star} < 5:
		add 1 to {_star}

	if {_star} is 1:
		set {_star.text} to "&f⚝&8⚝⚝⚝⚝ &7(&f1&7)"
	if {_star} is 2:
		set {_star.text} to "&f⚝⚝&8⚝⚝⚝ &7(&f2&7)"
	if {_star} is 3:
		set {_star.text} to "&f⚝⚝⚝&8⚝⚝ &7(&f3&7)"
	if {_star} is 4:
		set {_star.text} to "&f⚝⚝⚝⚝&8⚝ &7(&f4&7)"
	if {_star} is 5:
		set {_star.text} to "&f⚝⚝⚝⚝⚝ &7(&f&l5&7)"

	if {_id} is "attack":
		if "%tag ""attack_rise"" of nbt of {_item.1}%" parsed as number > "%tag ""attack_rise"" of nbt of {_item.2}%" parsed as number:
			set {_value_old} to "%tag ""attack_rise"" of nbt of {_item.1}%" parsed as number
		else:
			set {_value_old} to "%tag ""attack_rise"" of nbt of {_item.2}%" parsed as number
		set {_value} to random integer between {_value_old} and {_star} / 5 * {@attack_rise_max}
		if {_mode} is 1:
			set {_give} to potion named "&c攻撃力&fのエレメント &7(&f%{_star}%&7)" with lore "&7装備-エレメント &8| %{_star.text}%" and "&7評価: %element_score({_value} / {@attack_rise_max})%" and "" and "&c攻撃力 &c+%{_value} / 100%%%"
